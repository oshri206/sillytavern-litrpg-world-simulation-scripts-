{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Merchant",
  "type": "object",
  "required": ["id", "name", "inventory", "prices"],
  "properties": {
    "id": { "type": "string" },
    "name": { "type": "string" },
    "inventory": {
      "type": "array",
      "items": { "type": "string" }
    },
    "prices": {
      "type": "object",
      "additionalProperties": { "type": "number" }
    }
  }
}
